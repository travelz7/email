// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model Email {
    id        Int      @id @default(autoincrement())
    to        String?
    from      String?
    subject   String?
    body      String?
    code      String?
    createdAt DateTime @default(now())
}
